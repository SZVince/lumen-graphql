{
    "name": "beeeye/lumen-graphql",
    "description": "Facebook GraphQL for lumen",
    "keywords": ["framework", "lumen", "graphql", "react"],
    "authors": [
        {
            "name": "vince",
            "email": "wlp808@gmail.com"
        }
    ],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "5.5.*|5.6.*|5.7.*",
        "webonyx/graphql-php": "~0.13.0"
    },
    "autoload": {
        "psr-0": {
            "BeeEye\\GraphQL\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BeeEye\\GraphQL\\ServiceProvider"
            ],
            "aliases": {
                "GraphQL": "BeeEye\\GraphQL\\Support\\Facades\\GraphQL"
            }
        }
    }
}
